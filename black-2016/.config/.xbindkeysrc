#keystate_capslock = enable



"xbindkeys_show"
   control+shift + q

#VolUp
"amixer -D pulse sset Master 5%+"
    m:0x0 + c:69
    F3 

#VolDown
"amixer -D pulse sset Master 5%-"
    m:0x0 + c:68
    F2 

#Mute
"amixer sset Master toggle"
    m:0x0 + c:67
    F1 

#Lock
"~/.bin/lock.sh"
    m:0x40 + c:46
    Mod4 + l 

#Next song
"~/.bin/sp.sh next"
    m:0x0 + c:96
    F12 

#Previous song
"~/.bin/sp.sh prev"
    m:0x0 + c:76
    F10 

#Pause song
"~/.bin/sp.sh play"
    m:0x0 + c:95
    F11 

